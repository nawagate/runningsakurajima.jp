<mt:SetVarBlock name="thispage">TOP</mt:SetVarBlock>

<body id="sitetop">
<script>
(function() {
  var id = location.hash.replace('#', '');
  if (id) {
    var div = document.createElement('div'),
      a = document.createElement('a');
    a.setAttribute('name', id);
    div.appendChild(a);
    document.write(div.innerHTML);
  }
})();
</script>
